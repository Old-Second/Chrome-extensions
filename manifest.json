	"description":"大伙都爱学习QAQ",
    "action":{
        "default_popup":"html/popup.html"
	},
	"icons":{
		"16":"img/2.png",
		"32":"img/2.png",
		"48":"img/2.png",
		"128":"img/2.png"
	},
	"background":{
		"service_worker":"background.js"
	},
	"permissions":["storage", "cookies"],
	"content_scripts":[{
		"js":["js/content.js"],
		"matches":["http://kjyy.ccnu.edu.cn/*"],
		"run_at":"document_idle"
	}]
}
